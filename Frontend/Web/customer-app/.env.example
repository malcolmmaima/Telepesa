# API Configuration
# Backend services typically run on different ports
# Account service: 8082, Transaction service: 8083, etc.
VITE_API_BASE_URL=http://localhost:8082/api/v1

# App Configuration
VITE_APP_NAME=Telepesa
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature flags
VITE_ENABLE_LOANS=true
VITE_ENABLE_INVESTMENTS=false
VITE_ENABLE_ANALYTICS=true

# Third-party integrations
VITE_GOOGLE_ANALYTICS_ID=
VITE_SENTRY_DSN=

# Development settings
VITE_DEV_TOOLS=true
